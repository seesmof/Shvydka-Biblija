{
  "manifest_version": 2,
  "name": "Shvydka Biblija",
  "version": "0.1.2",
  "permissions": ["activeTab"],
  "commands": {
    "eBible": {
      "suggested_key": "Alt+Z",
      "description": "eBible"
    },
    "TSK": {
      "suggested_key": "Alt+C",
      "description": "TSK"
    },
    "BollsLife": {
      "suggested_key": "Alt+A",
      "description": "BollsLife"
    },
    "InstantBible": {
      "suggested_key": "Alt+Q",
      "description": "InstantBible"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "cross.png"
  },
  "icons": {
    "16": "cross.png",
    "48": "cross.png",
    "128": "cross.png"
  }
}
